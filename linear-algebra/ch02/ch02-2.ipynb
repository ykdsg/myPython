   "cell_type": "code",
   "source": [
    "A = np.array([[1, 35, 0],\n",
    "              [0, 2, 3],\n",
    "              [0, 0, 4]])\n",
    "\n",
    "print(np.linalg.matrix_rank(A))\n",
    "\n",
    "# linalg.inv计算逆矩阵\n",
    "A_n = linalg.inv(A)\n",
    "print(A_n)\n",
    "print(np.dot(A, A_n))"
   ],
